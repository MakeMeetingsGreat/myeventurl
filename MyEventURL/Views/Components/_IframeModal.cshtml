<!-- Modal -->
<div class="modal fade" id="iframemodal" role="dialog">
    <div class="modal-dialog modal-lg">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">{{Title}}</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body" style="height:600px">
                <iframe style="border:0px; width:100%; height:570px;" :src="Src" name="iframe"></iframe>
            </div>
        </div>
    </div>
</div>
<!-- Modal -->

<script>
    var iframemodal = new Vue({
        el: "#iframemodal",
        data: {
            Title: '',
            Src: '',
        },
        methods: {
            setTitle: function (title) { this.Title = title; },
            setSrc: function (src) { this.Src = src; }
        }
    })
</script>
